{
  "name": "scratch-svg-renderer",
  "version": "0.0.0-development",
  "description": "SVG renderer for Scratch",
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run clean && webpack --progress --colors --bail",
    "clean": "rimraf ./dist",
    "prepare": "husky install",
    "start": "webpack-dev-server",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "eslint . --ext .js",
    "test:unit": "tap ./test/*.js",
    "watch": "webpack --progress --colors --watch",
    "semantic-release": "semantic-release"
  },
  "author": "Massachusetts Institute of Technology",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/LLK/scratch-svg-renderer#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/LLK/scratch-svg-renderer.git"
  },
  "peerDependencies": {
    "scratch-render-fonts": "^1.0.0-prerelease"
  },
  "dependencies": {
    "base64-js": "1.2.1",
    "base64-loader": "1.0.0",
    "dompurify": "2.2.7",
    "minilog": "3.1.0",
    "transformation-matrix": "1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/travis-cli": "^12.1.4",
    "babel-core": "6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.6.1",
    "commitizen": "^4.2.4",
    "copy-webpack-plugin": "^4.5.1",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.13.0",
    "eslint-config-scratch": "^5.0.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^6.0.0",
    "jsdom": "^13.0.0",
    "json": "^9.0.6",
    "lodash.defaultsdeep": "4.6.1",
    "mkdirp": "^1.0.3",
    "rimraf": "^3.0.1",
    "scratch-render-fonts": "^1.0.0-prerelease",
    "semantic-release": "^17.4.3",
    "tap": "^11.0.1",
    "webpack": "^4.8.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4",
    "xmldom": "^0.1.27"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
